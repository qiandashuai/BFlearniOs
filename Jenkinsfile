pipeline {
	agent any

	stages {
		stage('Clone Code') {
			steps {
				echo "==== this is Clone Code ==="
			}
		}

		stage('Pod Install') {
			steps {
				echo "=== this is Pod Install ==="
			}
		}

		stage('Build') {
			steps {
				echo "=== this is Build ==="
			}
		}

		stage('Deploy') {
			steps {
				echo "=== this is Deploy ==="
			}
		}
	}
}